int calculateMinPatforms(int at[], int dt[], int n) {
    // Write your code here.
    int count[2361]={0};
    for(int i=0;i<n;i++){
        count[at[i]]++;
        count[dt[i]+1]--;
    }
    int temp=0;
    int res=0;
    for(int i=0;i<2361;i++){
        temp+=count[i];
        res=max(res,temp);
    }
    return res;
}
